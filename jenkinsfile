pipeline {
    agent any
    stages {
        stage( "clone code"){
            steps{
                git credentialsID: 'git_credentials', url: 'https://github.com/arunprabhavathi456/springboot-webapplication.git' 
            }
        }
        stage( "build code"){
            steps{
               sh " mvn clean install "
            }
        }
    }    
